{# #
 # Logo with edition name
#}
<a href="{{ xurl() }}" class="logo-with-edition-name">
  <div class="logo">
    {{ this.getSVGImage(this.getLogoPath())|raw }}
  </div>
  <div class="edition">
    {% if this.isRootAdmin() %}
      <span class="core-edition-name">
        {{ t('X-Cart') }}
      </span>
    {% else %}
      {{ t('X-Cart') }}
    {% endif %}
  </div>
</a>
